    "concurrently": "^3.5.1",
    "cookie-session": "^2.0.0-beta.3",
    "cors": "^2.8.4",
    "express": "^4.16.2",
    "google-id-token-verifier": "^0.2.3",
    "mongoose": "^5.0.16",
    "passport": "^0.4.0",
    "passport-google-id-token": "^0.4.5",
    "request": "^2.85.0"
  }
}
